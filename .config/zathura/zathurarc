set selection-clipboard clipboard
set default-fg "#c0c5ce"
set font "monospace normal 14"
set guioptions "sv"
set synctex true
set statusbar-h-padding 0
set statusbar-v-padding 0
set page-padding 1
map u scroll half-up
map d scroll half-down
map D toggle_page_mode
map r reload
map R rotate
map K zoom in
map J zoom out
map i recolor
map p print
set recolor
set recolor-lightcolor "#2b303b"
set recolor-darkcolor "#c0c5ce"
set default-bg "#232830"
set recolor-keephue
